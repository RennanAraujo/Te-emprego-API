{
    "env": {
        "browser": true,
        "es2021": true
    },
    "extends": "eslint:recommended",
    "parserOptions": {
        "ecmaVersion": "latest",
        "sourceType": "module"
    },
    "rules": {
        "consistent-return": "off",
        "no-console": "off",
        "imported/no-unresolved": "off",
        "no-underscore-dangle": "off",
        "prefer-promise-reject-errors": "off",
        "func-names": "off",
        "new-cap": "off",
        "no-shadow": "off",
        "no-unused-expressions": "off",
        "no-return-assign": "off"

    }
}
